RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.html [NC,L]

RewriteEngine On
ErrorDocument 404 /404


RewriteEngine On
RewriteCond %{HTTP_HOST} ^marchofthedroids.co.uk$
RewriteRule (.*) http://www.marchofthedroids.co.uk/$1 [R=301,L] 

RedirectMatch 301 ^/whats-on/(.*)$ http://marchofthedroids.co.uk/
